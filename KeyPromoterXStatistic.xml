<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close">
        <Item>
          <IdeaActionID>CloseContent</IdeaActionID>
          <ShortCut>'Ctrl+4'</ShortCut>
          <Description>Close</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Cut">
        <Item>
          <IdeaActionID>$Cut</IdeaActionID>
          <ShortCut>'Ctrl+X' or 'Shift+Delete'</ShortCut>
          <Description>Cut</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'REPL for consumption-charges-api'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'REPL for consumption-charges-api'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'Alt+Shift+7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>19</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>GoTo</Description>
          <Count>60</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'Ctrl+V' or 'Shift+Insert'</ShortCut>
          <Description>Paste</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'Shift+F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>